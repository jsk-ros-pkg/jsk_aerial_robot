<?xml version="1.0"?>
<robot
    xmlns:xacro="http://www.ros.org/wiki/xacro" name="gimbalrotor" >

  <xacro:include filename="$(find gimbalrotor)/urdf/bi/gimbalrotor.urdf.xacro" />
  <xacro:include filename="$(find gimbalrotor)/urdf/bi/common.xacro" />

  <xacro:gimbalrotor_center_link child1="1" child2="2"/>
  <xacro:gimbalrotor_thrust_link self="1" rotor_direction="1"/>
  <xacro:gimbalrotor_thrust_link self="2" rotor_direction="-1"/>

</robot>
