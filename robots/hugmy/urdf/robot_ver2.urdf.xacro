      <geometry>
        <mesh filename="package://hugmy/urdf/mesh/cable_carrier_arm_end.STL" />
      </geometry>
    </visual>
    </link>

    <!-- joint -->
    <joint name="joint_${id}_1" type="revolute">
      <parent link="main_body"/>
      <child link="link_${id}_1"/>
      <origin rpy="0 0 ${3*pi/4 - pi/2*(id-1)}" xyz="${base_offset}"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="0.8727" effort="10" velocity="1.0"/>
      <!-- <dynamics damping="0.0005" friction="0.8"/> -->
      <dynamics damping="0.002" friction="0.2"/>
    </joint>

    <joint name="joint_${id}_2" type="revolute">
      <parent link="link_${id}_1"/>
      <child link="link_${id}_2"/>
      <origin rpy="0 0 0" xyz="-${link_length_n} 0.00 0.00"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="1.047" effort="10" velocity="1.0"/>
      <!-- <dynamics damping="0.0005" friction="0.8"/> -->
      <dynamics damping="0.002" friction="0.2"/>
    </joint>

    <joint name="joint_${id}_3" type="revolute">
      <parent link="link_${id}_2"/>
      <child link="link_${id}_rotor"/>
      <origin rpy="0 0 0" xyz="-${link_length_n} 0.00 0.00"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="1.047" effort="10" velocity="1.0"/>
      <!-- <dynamics damping="0.0005" friction="0.8"/> -->
      <dynamics damping="0.002" friction="0.2"/>
    </joint>

    <joint name="joint_${id}_4" type="revolute">
