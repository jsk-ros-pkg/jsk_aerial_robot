        <mass value="0.00639991"/>
        <inertia
            ixx="0.00000153" ixy="0.00000000" ixz="-0.00000044"
            iyy="0.00000182" iyz="0.00000000"
            izz="0.00000214"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
