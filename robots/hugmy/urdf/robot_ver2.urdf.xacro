      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="1.047" effort="10" velocity="1.0"/>
      <!-- <dynamics damping="0.0005" friction="0.8"/> -->
      <dynamics damping="0.002" friction="0.2"/>
    </joint>

    <joint name="joint_${id}_5" type="revolute">
      <parent link="link_${id}_4"/>
      <child link="link_${id}_end"/>
      <origin rpy="0 0 0" xyz="-${link_length_n} 0.00 0.00"/>
      <axis xyz="0 -1 0"/>
      <limit lower="0" upper="1.047" effort="10" velocity="1.0"/>
      <!-- <dynamics damping="0.0005" friction="0.8"/> -->
      <dynamics damping="0.002" friction="0.2"/>
    </joint>

    <transmission name="joint_${id}_1_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="joint_${id}_1">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_${id}_servo_1">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    <transmission name="joint_${id}_2_tran">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="joint_${id}_2">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="joint_${id}_2_servo">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
