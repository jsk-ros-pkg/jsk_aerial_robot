        <origin xyz="-0.00910082 0.000000 0.00845129" rpy="0 0 0"/>
        <mass value="0.00430071"/>
        <inertia
            ixx="0.00000112" ixy="0.00000000" ixz="-0.00000037"
            iyy="0.00000107" iyz="0.00000000"
            izz="0.00000109"/>
