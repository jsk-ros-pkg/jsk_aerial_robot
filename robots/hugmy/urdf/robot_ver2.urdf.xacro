        <mesh filename="package://hugmy/urdf/mesh/cable_carrier_2.STL" />
        </geometry>
      </visual>
    </link>

    <link name="link_${id}_2">
      <inertial>
        <origin xyz="-0.01174501 0.0000000 0.00608468" rpy="0 0 0"/>
