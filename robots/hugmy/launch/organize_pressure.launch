<?xml version="1.0"?>
<launch>
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node" output="screen">
    <param name="port" value="/dev/ttyUSB1" />
    <param name="baud" value="57600" />
  </node>
  <node pkg="hugmy" type="air_pressure_control_node" name="air_pressure_control_node" output="screen">
    <param name="external_mode" value="true"/>
    <param name="control_rate_hz" value="50"/>
  </node>
</launch>
