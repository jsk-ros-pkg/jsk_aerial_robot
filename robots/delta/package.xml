<?xml version="1.0"?>
<package>
  <name>delta</name>
  <version>1.3.2</version>
  <description>The rolling robot package</description>

  <maintainer email="chou@jsk.imi.i.u-tokyo.ac.jp">Bakui Chou</maintainer>
  <license>BSD</license>
  <url type="website">https://github.com/jsk-ros-pkg/jsk_aerial_robot</url>
  <author email="sugihara@jsk.imi.i.u-tokyo.ac.jp">Kazuki Sugihara</author>

  <buildtool_depend>catkin</buildtool_depend>


  <build_depend>aerial_robot_control</build_depend>
